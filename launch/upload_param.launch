<?xml version="1.0"?>
<launch>

	<arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>

	<!-- send ROBOT parameters to parameter server -->
	<rosparam command="load" ns="/script_server/arm_left" file="$(find hmi_2015)/config/$(arg robot)/arm_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/arm_right" file="$(find hmi_2015)/config/$(arg robot)/arm_right_joint_configurations.yaml"/>

</launch>
